#!/bin/sh
for arg in "$@"; do
 case "$arg" in -l*|-L*|-shared|-static)
  exec "$TOOLCHAIN_ROOT_DIR/bin/arm-linux-gnueabi-gcc-5.3.1" -Os -pipe -march=armv7-a -mtune=cortex-a7 -mfpu=neon -mglibc -msoft-float -I$TOOLCHAIN_ROOT_DIR/usr/include -I$TOOLCHAIN_ROOT_DIR/include -L$TOOLCHAIN_ROOT_DIR/usr/lib -L$TOOLCHAIN_ROOT_DIR/lib ${STAGING_DIR:+-idirafter "$STAGING_DIR/usr/include" -L "$STAGING_DIR/usr/lib" -Wl,-rpath-link,"$STAGING_DIR/usr/lib"} "$@" ;;
 esac
done
exec "$TOOLCHAIN_ROOT_DIR/bin/arm-linux-gnueabi-gcc-5.3.1" -Os -pipe -march=armv7-a -mtune=cortex-a7 -mfpu=neon -mglibc -msoft-float -I$TOOLCHAIN_ROOT_DIR/usr/include -I$TOOLCHAIN_ROOT_DIR/include -L$TOOLCHAIN_ROOT_DIR/usr/lib -L$TOOLCHAIN_ROOT_DIR/lib ${STAGING_DIR:+-idirafter "$STAGING_DIR/usr/include"} "$@"
